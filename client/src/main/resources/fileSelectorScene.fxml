<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<BorderPane stylesheets="fileselector.css" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="400.0"
            prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <bottom>
        <HBox alignment="CENTER" prefHeight="62.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <children>
                <Button layoutX="266.0" layoutY="28.0" mnemonicParsing="false" onAction="#cancel" prefHeight="26.0"
                        prefWidth="88.0" text="Cancel" textAlignment="CENTER"/>
            </children>
        </HBox>
    </bottom>
    <center>
        <ListView fx:id="file_view" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER"
                  onMouseClicked="#handleClick"/>
    </center>
</BorderPane>
